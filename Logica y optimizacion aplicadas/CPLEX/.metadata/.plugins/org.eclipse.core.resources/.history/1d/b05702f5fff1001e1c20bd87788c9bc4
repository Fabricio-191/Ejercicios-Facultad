dvar float+ amoniaco;
dvar float+ cloruro_amonico;

// Funci√≥n objetivo
maximize 40 * amoniaco + 50 *cloruro_amonico;
// Restricciones
subject to
{
amoniaco+cloruro_amonico <= 50;
3*amoniaco+4*cloruro_amonico <= 180;
cloruro_amonico <= 40;
}